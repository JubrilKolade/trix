{
    "name": "angular-base",
    "type": "base",
    "framework": "angular",
    "files": [
        {
            "source": "frontend/base/angular/src/main.ts",
            "destination": "src/main.ts",
            "type": "template"
        },
        {
            "source": "frontend/base/angular/src/app/app.component.ts",
            "destination": "src/app/app.component.ts",
            "type": "template"
        },
        {
            "source": "frontend/base/angular/src/app/app.component.html",
            "destination": "src/app/app.component.html",
            "type": "template"
        },
        {
            "source": "frontend/base/angular/src/app/app.config.ts",
            "destination": "src/app/app.config.ts",
            "type": "template"
        },
        {
            "source": "frontend/base/angular/src/index.html",
            "destination": "src/index.html",
            "type": "template"
        },
        {
            "source": "frontend/base/angular/angular.json",
            "destination": "angular.json",
            "type": "template"
        },
        {
            "source": "frontend/base/angular/tsconfig.json",
            "destination": "tsconfig.json",
            "type": "template"
        }
    ],
    "dependencies": {
        "prod": {
            "@angular/animations": "^17.0.0",
            "@angular/common": "^17.0.0",
            "@angular/compiler": "^17.0.0",
            "@angular/core": "^17.0.0",
            "@angular/forms": "^17.0.0",
            "@angular/platform-browser": "^17.0.0",
            "@angular/platform-browser-dynamic": "^17.0.0",
            "@angular/router": "^17.0.0",
            "rxjs": "~7.8.0",
            "tslib": "^2.3.0",
            "zone.js": "~0.14.0"
        },
        "dev": {
            "@angular-devkit/build-angular": "^17.0.0",
            "@angular/cli": "^17.0.0",
            "@angular/compiler-cli": "^17.0.0",
            "@types/node": "^18.18.0",
            "typescript": "~5.2.0"
        }
    },
    "scripts": {
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test"
    }
}